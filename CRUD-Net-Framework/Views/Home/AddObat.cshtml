@model CRUD_Net_Framework.Models.OBAT
@{
    ViewBag.Title = "AddObat";
}

<h2 class="text-center">AddObat</h2>

@using (Html.BeginForm("addObats", "Home", FormMethod.Post, new { id = "DialogForm" }))
{
    <div class="form-group">
        <label name="Name" class="control-label">Nama Obat</label>
        <input placeholder="Nama Obat" name="nama_obat" id="nama_obat" class="form-control" />
    </div>
    <div class="form-group">
        <label name="Name" class="control-label">Jenis Obat</label>
        <input placeholder="Jenis Obat" name="jenis_obat" id="jenis_obat" class="form-control" />
    </div>
    <div class="form-group">
        <label name="Name" class="control-label">Harga Obat</label>
        <input placeholder="Harga Obat" name="harga_obat" id="harga_obat" class="form-control" />
    </div>
    <button type="submit" onclick="showNotifikasiSederhana('Tambah data berhasil')" class="k-button">Simpan</button>

}

